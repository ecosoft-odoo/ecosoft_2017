	<queryString>
		<![CDATA[SELECT DISTINCT
	title,
	title_th,
	supplier_name,
	supplier_name_th,
	supplier_taxid,
	tax_payer,
	supplier_street,
	supplier_street2,
	supplier_township,
	supplier_district,
	supplier_zip,
	supplier_country,
	supplier_province,
	report_pnd_form.STATE,
	SUM (report_pnd_form_line.base) AS base_total,
	SUM (report_pnd_form_line.tax) AS tax_total
FROM
	report_pnd_form
INNER JOIN report_pnd_form_line ON report_pnd_form.voucher_id = report_pnd_form_line.voucher_id
WHERE
	report_pnd_form.wht_period_id IN (SELECT id FROM account_period WHERE fiscalyear_id = $P{fiscalyear_id})
	AND report_pnd_form.income_tax_form = $P{income_tax_form}
GROUP BY
	title,
	title_th,
	supplier_name,
	supplier_name_th,
	supplier_taxid,
	tax_payer,
	supplier_street,
	supplier_street2,
