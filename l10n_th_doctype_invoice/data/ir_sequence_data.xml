<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        # Customer Invoices
        <record id="doctype_customer_invoice_seq" model="ir.sequence">
            <field name="name">DOCTYPE: Customer Invoice</field>
            <field name="code"></field>
            <field name="prefix">CIV%(fy)s</field>
            <field eval="6" name="padding"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="implementation">no_gap</field>
            <field name="special_type">doctype</field>
        </record>

        # Supplier Invoices
        <record id="doctype_supplier_invoice_seq" model="ir.sequence">
